<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.ukdw.fruit_freshned_app">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_app_2_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_app_2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fruit_freshned_app">
        <activity
            android:name="id.ac.ukdw.fruit_freshned_app.InsertActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="id.ac.ukdw.fruit_freshned_app.EditActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="id.ac.ukdw.fruit_freshned_app.LoginScreenActivity"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="id.ac.ukdw.fruit_freshned_app.RegisterScreenActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="id.ac.ukdw.fruit_freshned_app.BuahItemList"
            android:exported="true"
            >
        </activity>
        <activity
            android:name="id.ac.ukdw.fruit_freshned_app.DetailActivity"
            android:exported="true"
            >
        </activity>
    </application>

</manifest>